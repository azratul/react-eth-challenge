.About-image
    border-radius: 50%
    border: 4px solid #888
    grid-column-start: 1
    grid-row-start: 1
    grid-row-end: 3
    height: 196px
    width: 196px

.About-title
    grid-column-start: 2
    grid-row-start: 1
    padding-top: 70px

.About-items
    grid-column-start: 2
    grid-row-start: 2

.About-items > ul
    list-style-type: none

.About-items > ul > li
    padding-right: 5px

.About-rss > li
    float: left
